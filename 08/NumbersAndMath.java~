public class NumbersAndMath {
	public static void main(String[] args) {
	System.out.println("Now I will count my money.");
	System.out.println("$20" + (25 + 30 - 5) );
	System.out.println("$100" + (50 - 10 + 40 * 5 % 7) );

	System.out.println("Now I'll count my cheques.");
	System.out.println( 3 + 2 + 1 - 5 + 4 % 2 - 1 / 4 + 6 );

	System.out.println( "Is it true that 6 + 3 < 4 - 5?" );

	System.out.println( 6 + 3 < 4 - 5 );

	System.out.println( "What is 6 + 3? " + ( 6 + 3 ) );
	System.out.println( "What is 4 - 5? " + ( 4 - 5 ) );

	System.out.println( "Oh, that's why it's false." );

	System.out.println( "How about some more." );

	System.out.println( "Is it greater? " + ( 7 > -3 ) );
	System.out.println( "Is it greater or equal? " + ( 7 >= -3 ) );
	System.out.println( "Is it less or equal? " + ( 7 <= -3 ) ); }
}
